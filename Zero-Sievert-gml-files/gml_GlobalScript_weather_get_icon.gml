function weather_get_icon(arg0)
{
    switch (arg0)
    {
        case "sun_1":
            return s_weather_icon_sun_1;
            break;
        
        case "cloud_1":
            return s_weather_icon_cloud_1;
            break;
        
        case "rain_1":
            return s_weather_icon_rain_1;
            break;
        
        case "rain_2":
            return s_weather_icon_rain_2;
            break;
        
        case "rain_3":
            return s_weather_icon_rain_3;
            break;
        
        case "rad_rain_1":
            return s_weather_icon_rad_rain_1;
            break;
        
        case "rad_rain_2":
            return s_weather_icon_rad_rain_2;
            break;
        
        case "rad_rain_3":
            return s_weather_icon_rad_rain_3;
            break;
        
        case "fog_1":
            return s_weather_icon_fog_1;
            break;
        
        case "fog_2":
            return s_weather_icon_fog_1;
            break;
        
        case "snow_1":
            return s_weather_icon_snow_1;
            break;
        
        case "snow_2":
            return s_weather_icon_snow_2;
            break;
        
        case "snow_3":
            return s_weather_icon_snow_2;
            break;
    }
}
