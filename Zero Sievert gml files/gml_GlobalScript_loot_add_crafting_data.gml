function loot_add_crafting_data(arg0, arg1, arg2, arg3, arg4, arg5, arg6)
{
    with (arg0)
    {
        crafting_skill_id = arg1;
        crafting_skill_level = arg2;
        crafting_module_id = arg3;
        crafting_module_level = arg4;
        crafting_items_array = arg5;
        crafting_item_quantity_array = arg6;
    }
}
